@model IEnumerable<ContentManager.Models.Category>

@{
    ViewBag.Title = "Edit Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div>
    <h4>Link</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.categoryName)
        </dt>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.categoryName)
                </td>
            </tr>
        }
        <dd>
            @Html.DisplayNameFor(model => model.categoryInformation)
        </dd>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.categoryInformation)
                </td>
            </tr>
        }
        <dt>
            @Html.DisplayNameFor(model => model.categoryVideoUrl)

        </dt>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.categoryVideoUrl)
                </td>
            </tr>
        }
        <dd>
            @Html.DisplayNameFor(model => model.categoryAudioUrl)
        </dd>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.categoryAudioUrl)
                </td>
            </tr>
        }
        <dt>
            @Html.DisplayNameFor(model => model.categoryImageUrl)

        </dt>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.categoryImageUrl)
                </td>
            </tr>
        }
    </dl>
</div>
<p>

    @Html.ActionLink("Back to List", "Index")
</p>

